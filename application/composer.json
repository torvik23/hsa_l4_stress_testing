{
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "fzaninotto/faker": "^1.5",
        "php-di/php-di": "^6.0",
        "predis/predis": "^1.1",
        "selective/basepath": "^2.1",
        "slim/psr7": "^1",
        "slim/slim": "^4.4",
        "symfony/cache": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
